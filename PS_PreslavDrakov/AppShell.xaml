<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PS_PreslavDrakov.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PS_PreslavDrakov.View"
    xmlns:views=""
    Shell.FlyoutBehavior="Disabled"
    Title="PS_PreslavDrakov">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="MainPage" />
    <FlyoutItem Route="HomePage" FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="Home" ContentTemplate="{DataTemplate views:HomePage}"/>
    </FlyoutItem>
</Shell>
